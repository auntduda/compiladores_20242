let
    integer n, result, i, n_usada1, n_usada2.
in
    read n;                 
    result := 1;            
    i := 0;
    while n_usada1 < n_usada2 do
        n_usada1 := n_usada1 + 1;
    end;
    while i < n do         
        result := result * i;
        i := i + 1;          
    end;
    write result;           
end